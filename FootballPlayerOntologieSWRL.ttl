@prefix : <http://www.modsem.org/ontologies/FootballPlayerOntology#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@base <http://www.modsem.org/ontologies/FootballPlayerOntology#> .

<http://www.modsem.org/ontologies/FootballPlayerOntology> rdf:type owl:Ontology ;
                                                           dc:creator "Luca Cena" ;
                                                           dc:date "05/02/2024" ;
                                                           dc:description "Ontologia finalizzata a descrivere le carriere individuali dei calciatori"@it ;
                                                           dc:title "Football Player Ontology"@en ;
                                                           vann:preferredNamespacePrefix "http://www.modsem.org/ontologies/FootballPlayerOntology#" ,
                                                                                         "mgo" ;
                                                           rdfs:label "FootballPlayerOntology" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.modsem.org/ontologies/FootballPlayerOntology/preferredNamedspaceUri
<http://www.modsem.org/ontologies/FootballPlayerOntology/preferredNamedspaceUri> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#note
<http://www.w3.org/2004/02/skos/core#note> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.modsem.org/ontologies/FootballPlayerOntology#affronta
:affronta rdf:type owl:ObjectProperty ,
                   owl:IrreflexiveProperty ;
          rdfs:domain :Giocatore ;
          rdfs:range :Giocatore ;
          rdfs:comment "un giocatore affronta un altro giocatore se giocano stessa partita"@it ;
          rdfs:label "affronta"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#carrieraInCompetizione
:carrieraInCompetizione rdf:type owl:ObjectProperty ;
                        rdfs:domain :Giocatore ;
                        rdfs:range :Competizione ;
                        owl:propertyChainAxiom ( :haGiocatoPartita
                                                 :partitaInCompetizione
                                               ) ;
                        rdfs:comment "un giocatore ha giocato in una competizione, attraverso le due proprietà haGiocatoPartita e PartitaInCompetizione"@it ;
                        rdfs:label "carriera in competizione"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#giocataIn
:giocataIn rdf:type owl:ObjectProperty ,
                    owl:AsymmetricProperty ;
           rdfs:domain :Competizione ;
           rdfs:range :Mondo ;
           rdfs:comment "in che luogo si gioca una competizione"@it ;
           rdfs:label "giocata in"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#haGiocatoIn
:haGiocatoIn rdf:type owl:ObjectProperty ;
             owl:inverseOf :haGiocatore ;
             rdfs:domain :Giocatore ;
             rdfs:range :Squadra ;
             rdfs:comment "indica in che squadra ha giocato un giocatore, inversa di haGiocatore"@it ;
             rdfs:label "ha giocato in"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#haGiocatoPartita
:haGiocatoPartita rdf:type owl:ObjectProperty ;
                  rdfs:domain :Giocatore ;
                  rdfs:range :Partita ;
                  rdfs:comment "specifica una particolare partita giocata da un giocatore"@it ;
                  rdfs:label "ha giocato partita"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#haGiocatore
:haGiocatore rdf:type owl:ObjectProperty ;
             rdfs:domain :Squadra ;
             rdfs:range :Giocatore ;
             rdfs:comment "una squadra ha dei giocatori"@it ;
             rdfs:label "ha giocatore"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#haRuolo
:haRuolo rdf:type owl:ObjectProperty ;
         rdfs:domain :Giocatore ;
         rdfs:range :Ruolo ;
         rdfs:comment "ogni giocatore ha un ruolo, obbligatoriamente tra portiere, difensore, centrocampista e attaccante"@it ;
         rdfs:label "ha ruolo"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#haValoreMercatoMaggiore
:haValoreMercatoMaggiore rdf:type owl:ObjectProperty ,
                                  owl:TransitiveProperty ;
                         rdfs:domain :Giocatore ;
                         rdfs:range :Giocatore ;
                         rdfs:comment "indica se giocatore ha valore di mercato maggiore di un altro"@it ;
                         rdfs:label "ha valore mercato maggiore"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#haVintoCompetizione
:haVintoCompetizione rdf:type owl:ObjectProperty ;
                     rdfs:domain :Giocatore ;
                     rdfs:range :Competizione ;
                     rdfs:comment "competizione vinta da un giocatore"@it ;
                     rdfs:label "ha vinto competizione"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#haVintoPremio
:haVintoPremio rdf:type owl:ObjectProperty ;
               rdfs:domain :Giocatore ;
               rdfs:range :Premio ;
               rdfs:comment "un giocatore vince premio"@it ;
               rdfs:label "ha vinto premio"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#partecipaACompetizione
:partecipaACompetizione rdf:type owl:ObjectProperty ;
                        rdfs:domain :Squadra ;
                        rdfs:range :Competizione ;
                        rdfs:comment "una squadra partecipa ad una competizione"@it ;
                        rdfs:label "partecipa a competizione"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#partitaInCompetizione
:partitaInCompetizione rdf:type owl:ObjectProperty ,
                                owl:FunctionalProperty ;
                       rdfs:domain :Partita ;
                       rdfs:range :Competizione ;
                       rdfs:comment "una partita giocata in una certa competizione"@it ;
                       rdfs:label "partita in competizione"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#posizioneInSquadra
:posizioneInSquadra rdf:type owl:ObjectProperty ;
                    rdfs:domain :Ruolo ;
                    rdfs:range :Squadra ;
                    rdfs:comment "è ruolo occupato da giocatore in squadra, all'interno della stessa squadra posso avere giocatori con stesso ruolo e un giocatore può aver ricoperto ruoli diversi in squadre diverse"@it ;
                    rdfs:label "posizione in squadra"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.modsem.org/ontologies/FootballPlayerOntology#anniCarriera
:anniCarriera rdf:type owl:DatatypeProperty ;
              rdfs:domain :Giocatore ;
              rdfs:range xsd:integer ;
              rdfs:comment "gli anni di carriera di un giocatore"@it ;
              rdfs:label "anni carriera"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#goalSegnati
:goalSegnati rdf:type owl:DatatypeProperty ;
             rdfs:domain :Giocatore ;
             rdfs:range xsd:integer ;
             rdfs:comment "il numero di gol segnati in carriera da giocatore"@it ;
             rdfs:label "goal segnati"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#nazionalità
:nazionalità rdf:type owl:DatatypeProperty ;
             rdfs:domain :Giocatore ;
             rdfs:range xsd:string ;
             rdfs:comment "la nazionalità di un giocatore"@it ;
             rdfs:label "nazionalità"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#nomeGiocatore
:nomeGiocatore rdf:type owl:DatatypeProperty ;
               rdfs:domain :Giocatore ;
               rdfs:range xsd:string ;
               rdfs:comment "il nome completo di un giocatore"@it ;
               rdfs:label "nome giocatore"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#trofeiVinti
:trofeiVinti rdf:type owl:DatatypeProperty ;
             rdfs:domain :Giocatore ;
             rdfs:range xsd:integer ;
             rdfs:comment "somma dei trofei vinti da un giocatore"@it ;
             rdfs:label "trofei vinti"@it .


#################################################################
#    Classes
#################################################################

###  http://www.modsem.org/ontologies/FootballPlayerOntology#Attaccante
:Attaccante rdf:type owl:Class ;
            rdfs:subClassOf :Ruolo ;
            rdfs:comment "Ruolo di attaccante che giocatore può ricoprire"@it ;
            rdfs:label "Attaccante"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Campione_Assoluto
:Campione_Assoluto rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                owl:onProperty :haVintoPremio ;
                                                                owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                                owl:onClass :Premio
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :trofeiVinti ;
                                                                owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                     owl:onDatatype xsd:integer ;
                                                                                     owl:withRestrictions ( [ xsd:minExclusive 40
                                                                                                            ]
                                                                                                          )
                                                                                   ]
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:subClassOf :Top_player ;
                   rdfs:comment "giocatore che in carriera ha vinto almeno 2 premi e che in carriera ha vinto un numero di trofei maggiore di 40"@it ;
                   rdfs:label "Campione assoluto"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Centrocampista
:Centrocampista rdf:type owl:Class ;
                rdfs:subClassOf :Ruolo ;
                rdfs:comment "Ruolo di centrocampista che giocatore può ricoprire"@it ;
                rdfs:label "Centrocampista"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Competizione
:Competizione rdf:type owl:Class ;
              owl:equivalentClass <https://dbpedia.org/ontology/SoccerLeague> ;
              rdfs:comment "Coppe e/o campionati a cui più squadre partecipano"@it ;
              rdfs:label "Competizione"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Competizione_continentale
:Competizione_continentale rdf:type owl:Class ;
                           owl:equivalentClass [ rdf:type owl:Restriction ;
                                                 owl:onProperty :giocataIn ;
                                                 owl:someValuesFrom :Continente
                                               ] ;
                           rdfs:subClassOf :Competizione_mondiale ;
                           rdfs:comment "manifestazione sportiva giocata sul territorio del continente"@it ;
                           rdfs:label "Competizione continentale"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Competizione_mondiale
:Competizione_mondiale rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Restriction ;
                                             owl:onProperty :giocataIn ;
                                             owl:someValuesFrom :Mondo
                                           ] ;
                       rdfs:subClassOf :Competizione ;
                       rdfs:comment "manifestazione sportiva giocata a livello mondiale"@it ;
                       rdfs:label "Competizione mondiale"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Competizione_nazionale
:Competizione_nazionale rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Restriction ;
                                              owl:onProperty :giocataIn ;
                                              owl:someValuesFrom :Paese
                                            ] ;
                        rdfs:subClassOf :Competizione_continentale ;
                        rdfs:comment "manifestazione sportiva giocata a livello nazionale"@it ;
                        rdfs:label "Competizione nazionale"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Continente
:Continente rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Class ;
                                  owl:oneOf ( :Africa
                                              :America
                                              :Antartide
                                              :Asia
                                              :Europa
                                              :Oceania
                                            )
                                ] ;
            rdfs:subClassOf :Mondo ;
            rdfs:comment "Luogo fisico tra i continenti"@it ;
            rdfs:label "Continente"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Difensore
:Difensore rdf:type owl:Class ;
           rdfs:subClassOf :Ruolo ;
           rdfs:comment "Ruolo di difensore che giocatore può ricoprire"@it ;
           rdfs:label "Difensore"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Giocatore
:Giocatore rdf:type owl:Class ;
           owl:equivalentClass <https://dbpedia.org/ontology/SoccerPlayer> ;
           rdfs:comment "persona che gioca a calcio"@it ;
           rdfs:label "Giocatore"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Giocatore_Esperto
:Giocatore_Esperto rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                         owl:onProperty :anniCarriera ;
                                         owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                              owl:onDatatype xsd:integer ;
                                                              owl:withRestrictions ( [ xsd:minInclusive 10
                                                                                     ]
                                                                                   )
                                                            ]
                                       ] ;
                   rdfs:subClassOf :Giocatore ;
                   owl:disjointWith :Giocatore_Inesperto ;
                   rdfs:comment "giocatore che ha accumulato almeno 10 anni di carriera"@it ;
                   rdfs:label "Giocatore esperto"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Giocatore_Inesperto
:Giocatore_Inesperto rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty :anniCarriera ;
                                           owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                owl:onDatatype xsd:integer ;
                                                                owl:withRestrictions ( [ xsd:maxExclusive 10
                                                                                       ]
                                                                                     )
                                                              ]
                                         ] ;
                     rdfs:subClassOf :Giocatore ;
                     rdfs:comment "giocatore che ha accumulato meno di 10 anni di carriera"@it ;
                     rdfs:label "Giocatore inesperto"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Goleador
:Goleador rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                       owl:onProperty :haRuolo ;
                                                       owl:someValuesFrom :Attaccante
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :goalSegnati ;
                                                       owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                            owl:onDatatype xsd:integer ;
                                                                            owl:withRestrictions ( [ xsd:minExclusive 250
                                                                                                   ]
                                                                                                 )
                                                                          ]
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf :Giocatore ;
          rdfs:comment "giocatore che gioca come attaccante e ha segnato almeno 250 gol"@it ;
          rdfs:label "Goleador"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Goleador_Vincente
:Goleador_Vincente rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                owl:onProperty :haRuolo ;
                                                                owl:someValuesFrom :Attaccante
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :haVintoCompetizione ;
                                                                owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                                owl:onClass :Competizione
                                                              ]
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :goalSegnati ;
                                                                owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                     owl:onDatatype xsd:integer ;
                                                                                     owl:withRestrictions ( [ xsd:minExclusive 250
                                                                                                            ]
                                                                                                          )
                                                                                   ]
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:subClassOf :Goleador ;
                   rdfs:comment "giocatore che gioca come attaccante, ha segnato almeno 250 gol e ha vinto almeno due competizioni"@it ;
                   rdfs:label "Goieador vincente"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Mondo
:Mondo rdf:type owl:Class .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Paese
:Paese rdf:type owl:Class ;
       rdfs:subClassOf :Continente ;
       rdfs:comment "territorio fisicoi"@it ;
       rdfs:label "Paese"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Partita
:Partita rdf:type owl:Class ;
         owl:equivalentClass <https://dbpedia.org/ontology/FootballMatch> ;
         rdfs:comment "match in cui due squadre si affrontano"@it ;
         rdfs:label "Partita"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Partita_Campionato
:Partita_Campionato rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty :partitaInCompetizione ;
                                          owl:someValuesFrom :Competizione_nazionale
                                        ] ;
                    rdfs:subClassOf :Partita_Europea ;
                    rdfs:comment "partita riferita a competizione nazionale"@it ;
                    rdfs:label "Partita campionato"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Partita_Europea
:Partita_Europea rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :partitaInCompetizione ;
                                       owl:someValuesFrom :Competizione_continentale
                                     ] ;
                 rdfs:subClassOf :Partita ;
                 rdfs:comment "partita giocata su territorio europeo e riferita a competizione europea"@it ;
                 rdfs:label "Partita europea"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Portiere
:Portiere rdf:type owl:Class ;
          rdfs:subClassOf :Ruolo ;
          rdfs:comment "Ruolo di portiere che giocatore può ricoprire"@it ;
          rdfs:label "Portiere"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Premio
:Premio rdf:type owl:Class ;
        rdfs:comment "riconoscimento asseganto a giocatore"@it ;
        rdfs:label "Premio"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Ruolo
:Ruolo rdf:type owl:Class ;
       rdfs:comment "ruolo occupato dal giocatore, sarà portiere, difensore, centrocampista oppure attaccante"@it ;
       rdfs:label "Ruolo"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Squadra
:Squadra rdf:type owl:Class ;
         owl:equivalentClass <https://dbpedia.org/ontology/SoccerClub> ,
                             [ rdf:type owl:Class ;
                               owl:oneOf ( :Barcellona
                                           :Borussia_Dortmund
                                           :Inter
                                           :Juventus
                                           :Liverpool
                                           :Manchester_City
                                           :Manchester_United
                                           :Milan
                                           :Paris_Saint_Germain
                                           :Real_Madrid
                                         )
                             ] ;
         rdfs:comment "assocaciazione con finalità sportiva composta da giocatori"@it ;
         rdfs:label "Squadra"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#SquadraVincente
:SquadraVincente rdf:type owl:Class ;
                 rdfs:subClassOf :Squadra ;
                 rdfs:label "Squadra vincente"@it ,
                            "squadra che ha avuto giocatori che hanno vinto competizioni in carriera"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Top_player
:Top_player rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :haVintoPremio ;
                                  owl:someValuesFrom :Premio
                                ] ;
            rdfs:subClassOf :Giocatore ;
            rdfs:comment "giocatore che in carriera ha vinto almeno un premio"@it ;
            rdfs:label "Top player"@en .


###  https://dbpedia.org/ontology/FootballMatch
<https://dbpedia.org/ontology/FootballMatch> rdf:type owl:Class .


###  https://dbpedia.org/ontology/SoccerClub
<https://dbpedia.org/ontology/SoccerClub> rdf:type owl:Class .


###  https://dbpedia.org/ontology/SoccerLeague
<https://dbpedia.org/ontology/SoccerLeague> rdf:type owl:Class .


###  https://dbpedia.org/ontology/SoccerPlayer
<https://dbpedia.org/ontology/SoccerPlayer> rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  http://www.modsem.org/ontologies/FootballPlayerOntology#Africa
:Africa rdf:type owl:NamedIndividual ,
                 :Continente ;
        rdfs:comment "continente chiamato Africa"@it ;
        rdfs:label "Africa"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#America
:America rdf:type owl:NamedIndividual ,
                  :Continente ;
         rdfs:comment "continente chiamato America"@it ;
         rdfs:label "America"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Antartide
:Antartide rdf:type owl:NamedIndividual ,
                    :Continente ;
           rdfs:comment "continente chiamato Antartide"@it ;
           rdfs:label "Antartide"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Asia
:Asia rdf:type owl:NamedIndividual ,
               :Continente ;
      rdfs:comment "continente chiamato Asia"@it ;
      rdfs:label "Asia"@it .


###  http://www.modsem.org/ontologies/FootballPlayerOntology#Barcellona
:Barcellona rdf:type owl:NamedIndividual ;
            owl:sameAs <https://www.wikidata.org/wiki/Q7156> ;
            rdfs:comment "associazione calcistica chiamata Barcellona"@it ;
            rdfs:label "Barcellona"@it .


###  https://www.wikidata.org/wiki/Q7156

###  http://www.modsem.org/ontologies/FootballPlayerOntology#Barella
:Barella rdf:type owl:NamedIndividual ,
                  :Giocatore ;
         owl:sameAs <https://www.wikidata.org/wiki/Q20090225> ;
         :haGiocatoIn :Inter ;
         :haGiocatoPartita <http://www.modsem.org/ontologies/FootballPlayerOntology#Int_vs_Mil_23/24> ;
         :haRuolo :centrocampistaInter ;
         :haValoreMercatoMaggiore :Vlahovic ;
         :haVintoCompetizione :Europei_2021 ;
         :anniCarriera 10 ;
         :goalSegnati 33 ;
         :nazionalità "Italia" ;
         :nomeGiocatore "Nicolò Barella" ;
         :trofeiVinti 8 ;
         rdfs:comment "giocatore chiamato Barella"@it ;
         rdfs:label "Barella"@en .


###  https://www.wikidata.org/wiki/Q20090225

###  http://www.modsem.org/ontologies/FootballPlayerOntology#Bellingham
:Bellingham rdf:type owl:NamedIndividual ,
                     :Giocatore ;
            owl:sameAs <https://www.wikidata.org/wiki/Q66241169> ;
            :haGiocatoIn :Borussia_Dortmund ,
                         :Real_Madrid ;
            :haGiocatoPartita <http://www.modsem.org/ontologies/FootballPlayerOntology#Bar_vs_RealMad_23/24> ;
            :haRuolo :attaccanteRealMadrid ,
                     :centrocampistaBorussiaDortmund ,
                     :centrocampistaRealMadrid ;
            :anniCarriera 6 ;
